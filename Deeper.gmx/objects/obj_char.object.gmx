    dir = point_direction(0,0,joystick_xpos(0),joystick_ypos(0));

//Right
if dir &gt;        0
    if dir &lt;    45
        sprite_index = spr_body_alt
        
if dir &gt;        315
    if dir &lt;    360
        sprite_index = spr_body_alt
        
        
//Up
if dir &gt;        45
    if dir &lt;    135
        sprite_index = spr_body_alt
        
//Left
