        {
            if move != 1
                {
                image_index += 1
                move = 1
                }
            else    //Makes dat dust
                if !instance_exists(obj_dust)
                    instance_create(x,y,obj_dust)
            
            image_speed = 0.1
            //image_angle = point_direction(0,0,joystick_xpos(0),joystick_ypos(0));
            if! instance_position(x + joystick_xpos(0)*10,y,par_solid)
                x += joystick_xpos(0)*10;
            if! instance_position(x,y + joystick_ypos(0)*10,par_solid)
                y += joystick_ypos(0)*10;
        }
        else
            {
            image_speed = 0   
            move = 0
            }  
</string>
