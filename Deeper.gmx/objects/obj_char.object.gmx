        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement KEYBOARD

//Input
if keyboard_check(ord("W"))
    if momentum_y != -1
        momentum_y -= 0.1
if keyboard_check(ord("S"))
    if momentum_y != +1
        momentum_y += 0.1
if keyboard_check(ord("A"))
    if momentum_x != -1
        momentum_x -= 0.1
if keyboard_check(ord("D"))
    if momentum_x != +1
        momentum_x += 0.1

//Moves
x += momentum_x*10
y += momentum_y*10
    
//Punishes movement
if momentum_x &gt; 0
    momentum_x -= 0.05
if momentum_x &lt; 0
    momentum_x += 0.05
if momentum_y &gt; 0
    momentum_y -= 0.05
if momentum_y &lt; 0
    momentum_y += 0.05
</string>
